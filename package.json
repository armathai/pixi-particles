{
    "name": "@armathai/pixi-particles",
    "version": "1.0.1",
    "description": "Pixi Performance Monitor",
    "main": "bin/pixi-particles.js",
    "types": "bin/pixi-particles.d.ts",
    "scripts": {
        "test": "jest --config jestconfig.json",
        "start": "npx parcel src/__examples__/index.html",
        "make:dts": "node scripts/make_dts.js",
        "uglify": "node scripts/uglify.js",
        "build": "tsc && npm run make:dts && npm run uglify",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,ts}' --quiet --fix",
        "prepare": "npm run build --scripts-prepend-node-path",
        "prepublishOnly": "npm test && npm run lint"
    },
    "repository": {
        "url": "git@github.com:armathaigames/pixi-stats.git"
    },
    "keywords": [
        "pixijs5",
        "pixijs",
        "pixi.js",
        "pixi",
        "stats",
        "statsjs"
    ],
    "author": "armathai LLC",
    "license": "MIT",
    "homepage": "https://github.com/armathaigames/pixi-stats#readme",
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.2",
        "@types/stats.js": "^0.17.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^25.4.0",
        "line-reader": "^0.4.0",
        "parcel-bundler": "^1.12.4",
        "pixi.js": "^5.2.3",
        "prettier": "^1.19.1",
        "ts-jest": "^25.4.0",
        "tslint": "^6.0.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.7.4",
        "uglify-js": "^3.9.3"
    },
    "files": [
        "bin/**/*"
    ],
    "dependencies": {
        "stats.js": "^0.17.0"
    }
}
